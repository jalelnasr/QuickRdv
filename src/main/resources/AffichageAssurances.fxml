<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="472.0" prefWidth="605.0" stylesheets="@CSS/design.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controllers.AffichageAssurances">
    <children>
        <!-- Title for the CardView -->
        <Label layoutX="50" layoutY="20" style="-fx-font-size: 20px; -fx-font-weight: bold;" text="Liste des Assurances" />

        <!-- ScrollPane to hold the cards -->
        <ScrollPane layoutX="25.0" layoutY="46.0" prefHeight="400.0" prefWidth="554.0">
            <content>
                <!-- VBox to hold the cards -->
                <VBox fx:id="cardContainer" spacing="20" style="-fx-padding: 20;" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
